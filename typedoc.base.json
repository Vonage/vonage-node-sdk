{
  "$schema": "https://typedoc.org/schema.json",
  "includeVersion": false,
  "excludeExternals": true,
  "cleanOutputDir": true,
  "categorizeByGroup": true,
  "navigation": {
    "includeCategories": true,
    "includeGroups": false
  },
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false,
    "@alpha": false,
    "@beta": false
  },
  "groupOrder": ["Client"]
}
