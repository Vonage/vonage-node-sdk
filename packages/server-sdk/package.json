{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@vonage/server-sdk",
  "version": "3.24.1",
  "description": "Node.js Client for the Vonage API Platform",
  "homepage": "https://developer.vonage.com",
  "bugs": {
    "url": "https://github.com/Vonage/vonage-node-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vonage/vonage-node-sdk.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Chris Tankersley",
      "url": "https://github.com/dragonmantank"
    },
    {
      "name": "Chuck \"MANCHUCK\" Reeves",
      "url": "https://github.com/manchuck"
    }
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.cjs"
      },
      "browser": "./dist/unsupportedBrowser.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "postbuild": "node ../../scripts/copyUnsupported.js",
    "build:cjs": "tsc -p tsconfig.build.cjs.json && node -e \"const fs=require('node:fs');const p='dist/cjs';(function w(d){for(const e of fs.readdirSync(d,{withFileTypes:true})) e.isDirectory()?w(d+'/'+e.name):e.name.endsWith('.js')&&fs.renameSync(d+'/'+e.name,d+'/'+e.name.replace(/\\.js$/,'.cjs'));})(p)\"",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:types": "tsc -p tsconfig.build.types.json",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@vonage/accounts": "^1.17.1",
    "@vonage/applications": "^1.17.1",
    "@vonage/auth": "^1.12.0",
    "@vonage/conversations": "^1.9.3",
    "@vonage/messages": "^1.20.3",
    "@vonage/number-insights": "^1.17.2",
    "@vonage/numbers": "^1.19.0",
    "@vonage/pricing": "^1.17.1",
    "@vonage/redact": "^1.16.1",
    "@vonage/server-client": "^1.17.1",
    "@vonage/sms": "^1.18.1",
    "@vonage/users": "^1.11.1",
    "@vonage/verify": "^1.17.1",
    "@vonage/verify2": "^1.16.2",
    "@vonage/video": "^1.25.1",
    "@vonage/voice": "^1.18.1"
  },
  "engines": {
    "node": ">=20"
  },
  "engineStrict": true,
  "publishConfig": {
    "directory": "dist"
  }
}
