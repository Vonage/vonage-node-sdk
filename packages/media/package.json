{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@vonage/media",
  "version": "1.3.2-alpha.0",
  "description": "The Media API can be used to query, download and delete media items such as audio files for use with other Vonage APIs.",
  "homepage": "https://github.com/vonage/vonage-node-sdk/tree/main/packages/media#readme",
  "bugs": {
    "url": "https://github.com/Vonage/vonage-node-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vonage/vonage-node-sdk.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Chuck \"MANCHUCK\" Reeves",
      "url": "https://github.com/manchuck"
    }
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "browser": "./dist/unsupportedBrowser.cjs"
    },
    "./*": {
      "import": "./esm/*",
      "require": "./cjs/*"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "postbuild": "node ../../scripts/copyUnsupported.js",
    "build:cjs": "tsc -p tsconfig.build.cjs.json && node -e \"const fs=require('node:fs');const p='dist/cjs';(function w(d){for(const e of fs.readdirSync(d,{withFileTypes:true})) e.isDirectory()?w(d+'/'+e.name):e.name.endsWith('.js')&&fs.renameSync(d+'/'+e.name,d+'/'+e.name.replace(/\\.js$/,'.cjs'));})(p)\"",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:types": "tsc -p tsconfig.build.types.json",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@vonage/server-client": "^1.17.3-alpha.0"
  },
  "engines": {
    "node": ">=20"
  },
  "engineStrict": true
}
