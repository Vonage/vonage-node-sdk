{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@vonage/auth",
  "version": "1.12.0",
  "description": "Vonage Auth Package adds the correct authentication headers to requests to Vonage API's",
  "homepage": "https://github.com/vonage/vonage-node-sdk/tree/master/packages/auth#readme",
  "bugs": {
    "url": "https://github.com/Vonage/vonage-node-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vonage/vonage-node-sdk.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Kelly J Andrews",
      "url": "https://github.com/kellyjandrews"
    },
    {
      "name": "Chris Tankersley",
      "url": "https://github.com/dragonmantank"
    },
    {
      "name": "Chuck 'MANCHUCK' Reeves",
      "url": "https://github.com/manchuck"
    }
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.cjs"
      },
      "browser": "./dist/unsupportedBrowser.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:cjs": "tsc -p tsconfig.build.cjs.json && node -e \"const fs=require('node:fs');const p='dist/cjs';(function walk(d){for(const e of fs.readdirSync(d,{withFileTypes:true})) e.isDirectory()?walk(d+'/'+e.name):e.name.endsWith('.js')&&fs.renameSync(d+'/'+e.name,d+'/'+e.name.replace(/\\.js$/,'.cjs'));})(p)\"",
    "build:types": "tsc -p tsconfig.build.types.json",
    "postbuild": "node ../../scripts/copyUnsupported.js",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@vonage/jwt": "^1.11.0",
    "debug": "^4.4.1"
  },
  "devDependencies": {
    "@jest/globals": "30.1.2",
    "@types/node": "^24.3.1"
  },
  "engines": {
    "node": ">=20"
  },
  "engineStrict": true
}
