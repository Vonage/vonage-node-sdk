{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@vonage/jwt",
  "version": "1.11.2-alpha.7",
  "description": "Vonage JWT package. Creates JWT tokens for Vonage API's",
  "homepage": "https://github.com/vonage/vonage-node-sdk/tree/master/packages/jwt#readme",
  "bugs": {
    "url": "https://github.com/Vonage/vonage-node-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vonage/vonage-node-sdk.git"
  },
  "license": "Apache-2.0",
  "type": "module",
  "contributors": [
    {
      "name": "Kelly J Andrews",
      "url": "https://github.com/kellyjandrews"
    },
    {
      "name": "Chris Tankersley",
      "url": "https://github.com/dragonmantank"
    },
    {
      "name": "Chuck \"MANCHUCK\" Reeves",
      "url": "https://github.com/manchuck"
    }
  ],
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "browser": "./dist/unsupportedBrowser.cjs"
    },
    "./*": {
      "import": "./esm/*",
      "require": "./cjs/*"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsup lib/** -d dist/cjs",
    "build:cjs": "tsup lib/** -d dist/esm --format=esm",
    "build:types": "tsc -p tsconfig.build.types.json",
    "postbuild": "node ../../scripts/copyUnsupported.js",
    "clean": "rimraf dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "debug": "^4.4.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "9.0.10",
    "@types/uuid": "^10.0.0"
  },
  "engines": {
    "node": ">=20"
  },
  "engineStrict": true
}
